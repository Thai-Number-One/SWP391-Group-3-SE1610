<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>reservation</servlet-name>
        <servlet-class>controller_staff.reservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filterreservation</servlet-name>
        <servlet-class>controller_staff.filterreservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchreservation</servlet-name>
        <servlet-class>controller_staff.searchreservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reservationdetails</servlet-name>
        <servlet-class>controller_staff.reservationdetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>medical</servlet-name>
        <servlet-class>controller_staff.medical</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filtermedical</servlet-name>
        <servlet-class>controller_staff.filtermedical</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prescription</servlet-name>
        <servlet-class>controller_staff.prescription</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkprescription</servlet-name>
        <servlet-class>controller_staff.checkprescription</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedbackslist</servlet-name>
        <servlet-class>Control.feedbackslist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbacksFilter</servlet-name>
        <servlet-class>Control.FeedbacksFilter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reservation</servlet-name>
        <url-pattern>/reservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filterreservation</servlet-name>
        <url-pattern>/filterreservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchreservation</servlet-name>
        <url-pattern>/searchreservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reservationdetails</servlet-name>
        <url-pattern>/reservationdetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>medical</servlet-name>
        <url-pattern>/medical</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filtermedical</servlet-name>
        <url-pattern>/filtermedical</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prescription</servlet-name>
        <url-pattern>/prescription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkprescription</servlet-name>
        <url-pattern>/checkprescription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedbackslist</servlet-name>
        <url-pattern>/feedbackslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbacksFilter</servlet-name>
        <url-pattern>/feedbacksfilter</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
